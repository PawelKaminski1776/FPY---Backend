{
  "openapi": "3.0.1",
  "info": {
    "title": "Inspection-Backend-Process",
    "version": "1.0"
  },
  "paths": {
    "/Api/Accounts/AddNewAccount": {
      "post": {
        "tags": [
          "AddUser"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserCreationRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserCreationRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserCreationRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Api/Accounts/Login": {
      "post": {
        "tags": [
          "Login"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Api/Training/AddInspectionPattern": {
      "post": {
        "tags": [
          "Training"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Api/Training/Webscrape": {
      "post": {
        "tags": [
          "Websraper"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionImageRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionImageRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/InspectionImageRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "InspectionImageRequest": {
        "type": "object",
        "properties": {
          "inspectionName": {
            "type": "string",
            "nullable": true
          },
          "numOfImages": {
            "type": "integer",
            "format": "int32"
          },
          "county": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InspectionRequest": {
        "type": "object",
        "properties": {
          "inspectionName": {
            "type": "string",
            "nullable": true
          },
          "inspectionVariables": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UserCreationRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "company": {
            "type": "string",
            "nullable": true
          },
          "forename": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}